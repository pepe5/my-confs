# gconftool-2 -R /apps/metacity | egrep 'maximi| /' | grep -B1 maximi | head
 /apps/metacity/window_keybindings:
  maximize = disabled
  maximize_horizontally = <Mod4>w
  unmaximize = <Alt>F5
  toggle_maximized = <Mod4>x
  maximize_vertically = <Mod4>s
